#include <iostream>
#include <string>

using namespace std;

const int MAX_ANIMALS = 10000;
const int NUM_SPECIES = 8;
const string speciesGat[NUM_SPECIES] = {"bizon", "ara", "nosorozec", "hipopotam", "szympans", "gnu", "paw", "zebra"};

struct Animal {
    int weight;
    char name[11];
};

struct Species {
    Animal arr[MAX_ANIMALS];
    int size = 0;

    void insertionsort(const Animal &a) {
        int i = size - 1;

        while (i >= 0 && arr[i].weight > a.weight) {
            arr[i + 1] = arr[i];
            i--;
        }
        arr[i + 1] = a;
        size++;
    }

    void print() {
        if (size == 0) {
            cout << "-\n";
            return;
        }
        for (int i = 0; i < size; i++)
            cout << arr[i].name << ' ';
        cout << '\n';
    }

    void printReverse() {
        if (size == 0) {
            cout << "-\n";
            return;
        }
        for (int i = size - 1; i >= 0; i--)
            cout << arr[i].name << ' ';
        cout << '\n';
    }
};

int speciesIndex(const string &gat) {
    for (int i = 0; i < NUM_SPECIES; i++) {
        if (speciesGat[i] == gat)
            return i;
    }
    return -1;
}
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;
    Species species[NUM_SPECIES];

    for (int i = 0; i < n; i++) {
        string word1;
        string word2;
        cin >> word1;

        char c = word1[0];

        if (c == 'i' || c == 'r' || c == 'm') {
            cin >> word2;
            int count = speciesIndex(word2);
            if (count == -1) continue;

            if (c == 'i') {
                cout << species[count].size << '\n';
            } else if (c == 'r') {
                species[count].print();
            } else {
                species[count].printReverse();
            }
        }
        else {
            int weight;
            cin >> weight >> word2;
            int idx = speciesIndex(word1);
            if (idx == -1) continue;
            Animal a;
            a.weight = weight;
            int j = 0;
            for (; word2[j] && j < 10; j++) {
                a.name[j] = word2[j];
            }
            a.name[j] = '\0';

            species[idx].insertionsort(a);
        }
    }
    return 0;
}
