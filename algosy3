#include <iostream>
#include <string>

using namespace std;

struct Animal {
    string gatunek;
    int weight;
    string name;
};

int ile(const Animal *zbior, int animalsAmount, char firstLetter, const Animal *temp[] = nullptr) {
    int licznik = 0;
    for (int i = 0; i < animalsAmount; i++) {
        if (zbior[i].gatunek[0] == firstLetter) {
            if (temp) temp[licznik] = &zbior[i];
            licznik++;
        }
    }
    return licznik;
}

void insertionSortMal(const Animal *arr[], int n) {
    for (int i = 1; i < n; i++) {
        const Animal *key = arr[i];
        int j = i - 1;
        while (j >= 0 && arr[j]->weight <= key->weight) {
            arr[j + 1] = arr[j];
            j--;
        }
        arr[j + 1] = key;
    }
}

void insertionSortRos(const Animal *arr[], int n) {
    for (int i = 1; i < n; i++) {
        const Animal *key = arr[i];
        int j = i - 1;

            while (j >= 0 && arr[j]->weight > key->weight) {
                arr[j + 1] = arr[j];
                j--;
            }

        arr[j + 1] = key;
    }
}

void sortAndWrite(const Animal zbior[], int animalsAmount, char litera, bool ascending) {
    const Animal *temp[10000];
    int count = ile(zbior, animalsAmount, litera, temp);

    if (count == 0) {
        cout << "-\n";
        return;
    }
    if (ascending )
         insertionSortRos( temp,count);
    else {
        insertionSortMal(temp, count);
    }


    for (int i = 0; i < count; i++)
        cout << temp[i]->name << ' ';
    cout << '\n';
}


void ros(const Animal zbior[], int animalsAmount, char pierwszaLitera) {
    sortAndWrite(zbior, animalsAmount, pierwszaLitera, true);
}
void mal(const Animal zbior[], int animalsAmount, char pierwszaLitera) {
    sortAndWrite(zbior, animalsAmount, pierwszaLitera, false);
}


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    Animal zbiorGatunkow[10000];
    int animalsAmount = 0;

    for (int i = 0; i < n; i++) {
        char word1[50], word2[50];


        cin >> word1;


        if (word1[0] == 'i' || word1[0] == 'r' || word1[0] == 'm') {
            cin >> word2;
            char command = word1[0];
            char gatunek = word2[0];

            if (command == 'i')
                cout << ile(zbiorGatunkow, animalsAmount, gatunek) << '\n';
            else if (command == 'r')
                ros(zbiorGatunkow, animalsAmount, gatunek);
            else if (command == 'm')
                mal(zbiorGatunkow, animalsAmount, gatunek);
        }
        else {
            int weight = 0;

            cin >> weight >> word2;
            Animal a;
            a.gatunek = word1;
            a.weight = weight;
            a.name = word2;
            zbiorGatunkow[animalsAmount++] = a;
        }
    }

    return 0;
}
