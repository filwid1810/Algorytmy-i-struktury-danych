#include <iostream>

#include <string>
using namespace std;

struct Animal {
    string gatunek;
    int weight;
    string name;
};

int collectByLetter(const Animal* zbior, int animalsAmount, char litera, Animal* temp) {
    int count = 0;
    for (int i = 0; i < animalsAmount; i++) {
        if (zbior[i].gatunek[0] == litera) {
            temp[count++] = zbior[i];
        }
    }
    return count;
}

void ile(const Animal *zbior, int animalsAmount, char firstLetter) {
    int licznik = 0;
    for (int i = 0; i < animalsAmount; i++) {
        if (zbior[i].gatunek[0] == firstLetter) {
            licznik++;
        }
    }
    cout << licznik << '\n';
}



void ros(const Animal *zbior, int animalsAmount, char pierwszaLitera) {
Animal temp[10000];

   int count = collectByLetter(zbior, animalsAmount, pierwszaLitera, temp);

    if (count == 0) {
        cout <<"-\n";
        return;
    }




    for (int i = 1; i < count; i++) {
        Animal key = temp[i];
        int j = i - 1;
        while (j >= 0 && temp[j].weight > key.weight) {
            temp[j + 1] = temp[j];
            j--;
        }
        temp[j + 1] = key;
    }

    for (int i = 0; i < count; i++)
        cout << temp[i].name << ' ';
    cout << '\n';


}

void mal(const Animal *zbior, int animalsAmount, char pierwszaLitera) {

    Animal temp[10000];
    int count = collectByLetter(zbior, animalsAmount, pierwszaLitera, temp);

    if (count == 0) {
        cout <<'-'<<'\n';
        return;
    }

    
    for (int i = 1; i < count; i++) {
        Animal key = temp[i];
        int j = i - 1;
        while (j >= 0 && temp[j].weight <= key.weight) {
            temp[j + 1] = temp[j];
            j--;
        }
        temp[j + 1] = key;
    }

    for (int i = 0; i < count; i++)
        cout << temp[i].name << ' ';
    cout << '\n';


}


int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);


    string animals[] ={"bizon","ara","nosorozec","hipopotam","szympans","gnu","paw","zebra"};

   int n;
    cin >> n;
    cin.ignore();

    Animal zbiorGatunkow[10000];
    int animalsAmount = 0;

    for (int i = 0; i < n; i++) {
       string line;
        getline(cin, line);
    string words[3];

    int wordsAmount = 0;
    string actual;

    for (int j = 0; j<line.size(); j++) {
        if (line[j] == ' ') {
            if (!actual.empty()){
                words[wordsAmount++] = actual;
                actual = "";
            }
        }else {
            actual += line[j];
        }
    }

    if (!actual.empty()) {
        words[wordsAmount++] = actual;
    }

        if (wordsAmount == 3) {
            Animal animal;
            animal.gatunek = words[0];
            animal.weight = stoi(words[1]);
            animal.name = words[2];

            zbiorGatunkow[animalsAmount++] = animal;

        }

        else if (wordsAmount == 2) {
            char komenda = words[0][0];
            char gatunek = words[1][0];

            if (komenda == 'i') {
                ile(zbiorGatunkow, animalsAmount, gatunek);
                continue;
            }
            if (komenda == 'r') {
                        ros(zbiorGatunkow, animalsAmount, gatunek);
                        continue;
                    }
            if (komenda == 'm') {
                mal(zbiorGatunkow, animalsAmount, gatunek);
            }
        }
    }


    return 0;
}
