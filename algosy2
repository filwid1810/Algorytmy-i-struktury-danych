#include <iostream>
#include <cctype>
#include <string>


using namespace std;

string checksound(string sound);

bool isLetterAbleToBeMore(char x){
    x = tolower(x);
    if(x =='a'||x=='e' ||x=='i' ||x=='o'  ||x=='u' ){
        return 1;
    }
    else return 0;
}


string removeMoreLetters(string word){
    string wordWithoutMoreLetters = "";
    

    if (word.length() == 0) {
        return word;
    }

    wordWithoutMoreLetters += word[0];
    
    for(int i = 1; i<word.length();i++){
       char last = word[i-1];
       char curent = word[i];

         if (curent == last && isLetterAbleToBeMore(curent)) {
            continue;
        }  
        wordWithoutMoreLetters += word[i];
     }

     return wordWithoutMoreLetters;
}


string smallLetters(string word){
     for (int i = 0; i < word.length(); i++){
        word[i] = tolower(word[i]);
    } 
    return word;
}


string removeNotLetters(string word){
    string wordOnlyLetters = "";
    for(int i = 0; i<word.length();i++){
        if(isalpha(word[i])){
            wordOnlyLetters += word[i];
        }
    }
    return wordOnlyLetters;
}





string checksound(string sound){
    int zoo = 11;
   string sounds[] = {"ua","rar","io","mu","iha","tru","grr","pi","hu","be","kwa"};
   string animals[] ={"malpa","lew","osiol","bizon","kon","slon","tygrys","pingwin","sowa","owca","kaczka"};
   
    sound = removeNotLetters(sound);
    sound = smallLetters(sound);
    sound = removeMoreLetters(sound);


    for(int i=0; i<zoo;i++){
        if(sound == sounds[i]){
            return animals[i];
        }  
    } 
    return "nie wiem";
}


string isSpaceSomewere(string word){
    string current = "";
    string tab[50];
    int count = 0;

   
    for (int i = 0; i < word.length(); i++) {
        if (!isspace(word[i])) {
            current += word[i];
        } else if (current != "") {
            current = removeNotLetters(current);
            current = smallLetters(current);
            if (current != "") {
                tab[count++] = current;
            }
            current = "";
        }
    }
    
   if (current != "") {
        current = removeNotLetters(current);
        current = smallLetters(current);
        if (current != "") {
            tab[count++] = current;
        }
    }

     if (count == 0) return "nie wiem"; 


    string firstAnimal = checksound(tab[0]);

    for (int i = 1; i < count; i++) {
        string nextAnimal = checksound(tab[i]);
        if (nextAnimal != firstAnimal) {
            return "nie wiem"; 
        }
    }
     return firstAnimal;
}



int main(){
   int n;
    cin >> n;
    cin.ignore(); 

    string line;
    for (int i = 0; i < n; i++) {
        getline(cin, line);
        cout << isSpaceSomewere(line) << endl;
    }
    return 0;
}
