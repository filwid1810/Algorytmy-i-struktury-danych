#include <iostream>
#include <cctype>

using namespace std;

string removeNotLetters(string word){
    string wordOnlyLetters = "";
    for(int i = 0; i<word.length();i++){
        if(isalpha(word[i])){
            wordOnlyLetters += word[i];

        }
    }
    return wordOnlyLetters;
}

string checksound(string sound){
    int zoo = 11;
   string sounds[] = {"ua","rar","io","mu","iha","tru","grr","pi","hu","be","kwa"};
   string animals[] ={"malpa","lew","osiol","bizon","kon","slon","tygrys","pingwin","sowa","owca","kaczka"};
   
   sound = removeNotLetters(sound);

   for (int i = 0; i < sound.length(); i++){
        sound[i] = tolower(sound[i]);

    } 

    for(int i=0; i<zoo;i++){
        if(sound == sounds[i]){
            return animals[i];
        
        }
           
    } 
    return "nie wiem";
}



int main(){
    long n;//11 
    cin>>n;
    
    string word;
  
    string * tabl = new string[n];
    for(long i= 0;i<n;i++){
        cin>>word;
        
        tabl[i]= word;
    }
     for(long i= 0;i<n;i++){
        cout<<checksound(tabl[i])<<endl;
   
}
  
    delete[] tabl;

    return 0;
}
